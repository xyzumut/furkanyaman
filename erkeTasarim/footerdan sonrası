        jQuery(document).ready(function($) {

            const wg_menu_image = document.createElement('img');
                wg_menu_image.setAttribute(
                    'src',
                    'https://erketasarim.com/public/base/images/logo.svg',
                );

            $('#main-nav').hcOffcanvasNav({
            disableAt: 1024,
            customToggle: $('.mobil-toggle'),
            levelTitles: true,
            levelTitleAsBack: true,
            labelClose:       wg_menu_image,
            labelBack:        'Geri',
            disableBody:        false,
            width:'100%',
            });

        });

        $( ".mobil-toggle" ).click(function() {
            $("#hc-nav-1-1-0").remove();
            
        });
        
    </script>

